{
    "name": "jampire/oauth2-appid",
    "description": "IBM App ID OAuth 2.0 Client Provider for The PHP League OAuth2 Client",
    "homepage": "https://github.com/Jampire/oauth2-appid",
    "license": "MIT",
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on",
        "php",
        "api",
        "client",
        "ibm",
        "app id",
        "saml",
        "facebook",
        "google"
    ],
    "authors": [
        {
            "name": "Dzianis Kotau",
            "email": "me@dzianiskotau.com",
            "role": "Developer",
            "homepage": "https://dzianiskotau.com/"
        }
    ],
    "require": {
        "php": "^7.1",
        "league/oauth2-client": "^2.4"
    },
    "require-dev": {
        "ext-json": "*",
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^5.7|^6.0|^8.4",
        "mockery/mockery": "^1.3",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "jakub-onderka/php-console-highlighter": "^0.4.0",
        "squizlabs/php_codesniffer": "^2.3|^3.5"
    },
    "autoload": {
        "psr-4": {
            "Jampire\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jampire\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    }
}
